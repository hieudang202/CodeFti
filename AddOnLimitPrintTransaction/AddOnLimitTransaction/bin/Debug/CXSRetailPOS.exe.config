<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <configSections>
    <sectionGroup name="gibraltar">
      <section name="listener" type="Gibraltar.Agent.ListenerElement, Gibraltar.Agent" />
      <section name="packager" type="Gibraltar.Agent.PackagerElement, Gibraltar.Agent" />
      <section name="publisher" type="Gibraltar.Agent.PublisherElement, Gibraltar.Agent" />
      <section name="sessionFile" type="Gibraltar.Agent.SessionFileElement, Gibraltar.Agent" />
      <section name="viewer" type="Gibraltar.Agent.ViewerElement, Gibraltar.Agent" />
      <section name="email" type="Gibraltar.Agent.EmailElement, Gibraltar.Agent" />
      <section name="server" type="Gibraltar.Agent.ServerElement, Gibraltar.Agent" />
      <section name="autoSendConsent" type="Gibraltar.Agent.AutoSendConsentElement, Gibraltar.Agent" />
      <section name="networkViewer" type="Gibraltar.Agent.NetworkViewerElement, Gibraltar.Agent" />
      <section name="properties" type="System.Configuration.NameValueSectionHandler" />
    </sectionGroup>
  </configSections>

  <startup useLegacyV2RuntimeActivationPolicy="true">
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>


  <gibraltar>
    <!-- Here is where all of the Gibraltar configuration sections can be added.
For information on config options and common scenarios see
http://www.gibraltarsoftware.com/Support/Loupe/Documentation/WebFrame.html#DevelopersReference_AgentConfiguration_CommonScenarios.html
    -->
    <publisher productName="iVend" promotionLevelName="Dev" />
    <server autoSendSessions="True" sendAllApplications="True" useGibraltarService="True" customerName="iVendLogs" purgeSentSessions="false" />
    <listener catchApplicationExceptions="false" catchUnhandledExceptions="false" autoTraceRegistration="false" enableConsole="false" />
  </gibraltar>

  <system.diagnostics>
    <trace>
      <listeners>
        <add name="gibraltar" type="Gibraltar.Agent.LogListener, Gibraltar.Agent" />
      </listeners>
    </trace>
  </system.diagnostics>

  <system.serviceModel>
    <bindings>
      <basicHttpBinding>
        <binding name="basicHttpBinding_CXSSyncService" maxBufferSize="2147483647" maxReceivedMessageSize="2147483647" messageEncoding="Mtom" receiveTimeout="00:30:00" sendTimeout="00:30:00" transferMode="Streamed">
          <readerQuotas maxDepth="32" maxArrayLength="2147483647" maxStringContentLength="2147483647" />
          <!--<security mode="TransportCredentialOnly">
            <transport clientCredentialType="Basic"/>
          </security>-->
        </binding>
      
        <!-- Start - For Cayan -->
        <binding name="TransportServiceSoap">
          <security mode="Transport" />
        </binding>
        <binding name="TransportServiceSoap1" />
        <binding name="CreditSoap">
          <security mode="Transport" />
        </binding>
        <binding name="CreditSoap1" />
        <!-- End - For Cayan -->
      
      </basicHttpBinding>
    
     <customBinding>
        <!-- Start - For Cayan -->
        <binding name="CreditSoap12">
          <textMessageEncoding messageVersion="Soap12" />
          <httpsTransport />
        </binding>
        <!-- End - For Cayan -->
      </customBinding>

      <!-- For OPOS WCF service -->
      <netTcpBinding>
        <binding name="NetTcpBinding_IOPOSDuplexService" openTimeout="00:10:00" closeTimeout="00:10:00" sendTimeout="00:10:00" receiveTimeout="00:10:00" />
      </netTcpBinding>
      <!--End-->
    </bindings>
    <client>      
      <endpoint address="" binding="basicHttpBinding" bindingConfiguration="basicHttpBinding_CXSSyncService" contract="CXS.Platform.Synchronization.ISyncServiceV1" name="basicHttpBinding_CXSSyncService"></endpoint>
    
      <!-- Start - For Cayan -->
      <endpoint address="https://transport.merchantware.net/v4/transportService.asmx" binding="basicHttpBinding" bindingConfiguration="TransportServiceSoap" contract="TransportService.TransportServiceSoap" name="TransportServiceSoap" />
      <endpoint address="https://ps1.merchantware.net/Merchantware/ws/RetailTransaction/v4/Credit.asmx" binding="customBinding" bindingConfiguration="CreditSoap12" contract="CreditService.CreditSoap" name="CreditSoap12" />
      <!-- End - For Cayan -->

      <!--For OPOS WCF service-->
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/Printer/OPOSPrinterDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSPrinterService.IOPOSPrinterDuplexService" name="NetTcpBinding_IOPOSDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/MSR/OPOSMSRDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSMSRService.IOPOSMSRDuplexService" name="NetTcpBinding_IOPOSDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/Barcode/OPOSBarcodeDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSBarcodeService.IOPOSBarcodeDuplexService" name="NetTcpBinding_IOPOSBarcodeDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/PoleDisplay/OPOSPoleDisplayDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSPoleDisplayService.IOPOSPoleDisplayDuplexService" name="NetTcpBinding_IOPOSPoleDisplayDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/CashDrawer/OPOSCashDrawerDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSCashDrawerService.IOPOSCashDrawerDuplexService" name="NetTcpBinding_IOPOSDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/Scale/OPOSScaleDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSScaleService.IOPOSScaleDuplexService" name="NetTcpBinding_IOPOSDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <endpoint address="net.tcp://localhost:8400/CXS/OPOS/Services/CXS.Hardware.OPOS.Service/Endpoints/SignatureCapture/OPOSSignatureCaptureDuplexService/" binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IOPOSDuplexService" contract="OPOSSignatureCaptureService.IOPOSSignatureCaptureDuplexService" name="NetTcpBinding_IOPOSDuplexService">
        <identity>
          <dns value="localhost" />
        </identity>
      </endpoint>
      <!--End-->
    </client>
  </system.serviceModel>

  <runtime>

    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">

      <dependentAssembly>

        <assemblyIdentity name="System.Drawing" publicKeyToken="b03f5f7f11d50a3a" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-4.0.0.0" newVersion="4.0.0.0" />

      </dependentAssembly>

      <dependentAssembly>

        <assemblyIdentity name="CrystalDecisions.Shared" publicKeyToken="692fbea5521e1304" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-13.0.2000.0" newVersion="13.0.2000.0" />

      </dependentAssembly>

      <dependentAssembly>

        <assemblyIdentity name="CrystalDecisions.CrystalReports.Engine" publicKeyToken="692fbea5521e1304" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-13.0.2000.0" newVersion="13.0.2000.0" />

      </dependentAssembly>

      <dependentAssembly>

        <assemblyIdentity name="CrystalDecisions.ReportSource" publicKeyToken="692fbea5521e1304" culture="neutral" />

        <bindingRedirect oldVersion="0.0.0.0-13.0.2000.0" newVersion="13.0.2000.0" />

      </dependentAssembly>

    </assemblyBinding>

  </runtime>

  <appSettings>
    <add key="EnableHardwareLogging" value="false"/>
  </appSettings>
</configuration>
